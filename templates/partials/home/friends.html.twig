<div>
    <h2>Friends</h2>

    <ul class="list-reset flex flex-wrap">
        {% for friend in friends %}
            <li class="flex items-center w-1/6 md:w-1/5 lg:w-1/8">
                <a href="{{ friend.websiteUrl }}" title="{{ friend.name }}" class="mr-2 mb-2">
                    <img src="{{ asset(friend.logo) }}" alt="{{ friend.name }} logo">
                </a>
            </li>
        {% endfor %}
    </ul>
</div>
